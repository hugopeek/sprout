{
    "name": "hugopeek/sprout",
    "description": "Static markdown site generator for MODX.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "hugopeek",
            "email": "hugo@fractal-farming.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FractalFarming\\Sprout\\": "src/"
        }
    },
    "require": {
        "league/commonmark": "^2.2",
        "wa72/htmlpagedom": "2.0.*",
        "symfony/yaml": "^5.4",
        "symfony/polyfill-php80": "^1.26",
        "picocss/pico": "^1.5"
    },
    "repositories" : [{
        "type" : "package",
        "package" : {
            "name" : "picocss/pico",
            "version" : "1.5.0",
            "source" : {
                "url" : "https://github.com/picocss/pico",
                "type" : "git",
                "reference" : "master"
            }
        }
    }]
}
